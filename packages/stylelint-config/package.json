{
  "name": "@bigpopakap/stylelint-config",
  "version": "1.0.5",
  "description": "Stylelint configuration for bigpopakap's personal projects",
  "homepage": "https://github.com/bigpopakap/shared-node-tools",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bigpopakap/shared-node-tools"
  },
  "engines": {
    "node": ">=12.12"
  },
  "bin": {
    "stylelint-bigpopakap": "bin/stylelint-bigpopakap.js"
  },
  "scripts": {
    "lint": "yarn eslint-bigpopakap",
    "lintfix": "yarn lint --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "shelljs": "0.8.3",
    "stylelint": "12.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "3.1.1",
    "stylelint-processor-styled-components": "1.9.0",
    "yargs": "15.0.2"
  }
}
